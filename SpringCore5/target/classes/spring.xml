<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="car" class="com.tek.demo.Car">
		<property name="name" value="Honda" />
		<property name="price" value="600000"></property>
		<property name="engine" ref="engine" />
	</bean>

	<bean id="engine" class="com.tek.demo.Engine">
		<property name="name" value="Benz Engine" />
		<property name="enginecc" value="600" />
	</bean>
</beans>