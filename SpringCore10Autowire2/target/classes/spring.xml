<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean id="car" class="com.tek.demo.Car">
	</bean>
	<context:annotation-config />
	<bean id="engine2" class="com.tek.demo.Engine">
		<property name="name" value="Benz Engine" />
		<property name="enginecc" value="600" />
	</bean>
	<bean id="engine1" class="com.tek.demo.Engine">
		<property name="name" value="Audi Engine" />
		<property name="enginecc" value="900" />
	</bean>

</beans>